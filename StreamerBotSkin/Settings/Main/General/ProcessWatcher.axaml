<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:mControls="clr-namespace:Material.Styles.Controls;assembly=Material.Styles"
             x:Class="StreamerBotSkin.Controls.ProcessWatcher">
  <mControls:Card Padding="10">
    <StackPanel HorizontalAlignment="Center">
      <TextBlock>Process watcher</TextBlock>
      <CheckBox>Autostart</CheckBox>
      <Button>Start</Button>
    </StackPanel>
  </mControls:Card>
</UserControl>

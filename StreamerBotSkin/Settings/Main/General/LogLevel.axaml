<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:mAssists="clr-namespace:Material.Styles.Assists;assembly=Material.Styles"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="StreamerBotSkin.Controls.LogLevel">
  <ComboBox mAssists:ComboBoxAssist.Label="Log level"
            HorizontalAlignment="Center"
            SelectedIndex="0">
    <TextBlock>Verbose</TextBlock>
    <TextBlock>Debug</TextBlock>
    <TextBlock>Information</TextBlock>
  </ComboBox>
</UserControl>
